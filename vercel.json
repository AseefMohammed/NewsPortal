{
  "version": 2,
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "mobile/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "mobile/web-build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "mobile/web-build/$1"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  }
}
